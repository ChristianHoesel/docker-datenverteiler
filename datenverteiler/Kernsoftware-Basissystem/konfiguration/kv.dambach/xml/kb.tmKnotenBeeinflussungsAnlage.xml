<konfigurationsBereich pid="kb.tmKnotenBeeinflussungsAnlage" name="Teilmodell KnotenBeeinflussungsAnlage" verantwortlich="kv.dambach">
	<info>
		<kurzinfo>Teilmodell mit Datenmodellierung für Knotenbeeinflussungs-Anlagen.</kurzinfo>
	</info>
	<konfigurationsAenderung stand="22.04.2009" version="1" autor="Dambach-Werke GmbH, Th. Drapp" grund="Änderungsdokumentation siehe &apos;ReleaseNotes kv.dambach.doc&apos;"></konfigurationsAenderung>
	<konfigurationsAenderung stand="04.05.2009" version="2" autor="Dambach-Werke GmbH, Th. Drapp" grund="Änderungsdokumentation siehe &apos;ReleaseNotes kv.dambach.doc&apos;"></konfigurationsAenderung>
	<konfigurationsAenderung stand="29.03.2010" version="3" autor="Dambach-Werke GmbH, Th. Drapp" grund="Änderungsdokumentation siehe &apos;ReleaseNotes kv.dambach.doc&apos;"></konfigurationsAenderung>
