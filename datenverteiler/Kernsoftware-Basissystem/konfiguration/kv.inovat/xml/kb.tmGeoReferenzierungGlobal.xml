		<attributDefinition pid="att.länge" name="Länge">
			<info>
				<kurzinfo>Länge eines Linienobjekts.</kurzinfo>
			</info>
			<ganzzahl bits="64">
				<bereich skalierung="0,01" minimum="0" maximum="9223372036854775807" einheit="m">
				</bereich>
				<zustand name="undefiniert" wert="-1">
