		<attributDefinition pid="att.flächeXYReferenzOptional" name="FlächeXYReferenzOptional">
			<info>
				<kurzinfo>Attribut zur Referenzierung des Objekttyps: typ.flächeXY (Optionale Referenzierung erlaubt).</kurzinfo>
			</info>
			<objektReferenz typ="typ.flächeXY" undefiniert="erlaubt" referenzierungsart="assoziation"/>
