<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE konfigurationsBereich PUBLIC "-//K2S//DTD Dokument//DE" "K2S.dtd">
<konfigurationsBereich pid="kb.systemModellKExDaV" name="SystemdatenmodellKExDaV" verantwortlich="kv.kappich">
	<info>
		<kurzinfo>Datenmodell für KExDaV.</kurzinfo>
		<beschreibung>
			<absatz>Dieses Datenmodell spezifiziert Attributgruppen und Typen für die KExDaV-Anwendung
			</absatz>
		</beschreibung>
	</info>
	<!--
	Abhängigkeiten des Bereichs kb.systemModellKExDaV
	Abhängig ab Version      Art der Abhängigkeit      Version des benötigten Bereichs      Benötigter Bereich
	1                        notwendig                 1                                    kb.kv.kappich                      
	1                        notwendig                 27                                   kb.systemModellGlobal              
	1                        notwendig                 4                                    kb.fachModellGlobal                
	1                        notwendig                 3                                    kb.metaModellGlobal                
	1                        notwendig                 3                                    kb.tmVewBetriebGlobal              
	2                        notwendig                 30                                   kb.systemModellGlobal              
	-->
	<modell>
		<typDefinition pid="typ.kexdav" name="KExDaV" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Typ für Objekte, die KExDaV-Parameter erhalten</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.spezifikationKExDaV"/>
			<attributgruppe pid="atg.triggerKExDaV"/>
			<defaultParameter attributgruppe="atg.spezifikationKExDaV" typ="typ.kexdav">
				<datenliste name="Urlasser">
					<datum name="BenutzerReferenz" wert="undefiniert"/>
					<datum name="Ursache" wert=""/>
					<datum name="Veranlasser" wert=""/>
				</datenliste>
				<datenfeld name="RemoteDaV">
				</datenfeld>
			</defaultParameter>
		</typDefinition>
		<attributgruppenDefinition pid="atg.spezifikationKExDaV" name="SpezifikationKExDaV" parametrierend="ja">
			<info>
				<kurzinfo>Attributgruppe für KExDav-Parameter</kurzinfo>
			</info>
			<attributliste pid="atl.urlasser" anzahl="1" anzahlIst="fest" name="Urlasser">
			</attributliste>
			<attributliste pid="atl.remoteDaV" anzahl="0" anzahlIst="variabel" name="RemoteDaV">
				<info>
					<kurzinfo>Legt die Verbindungsparameter und die auszutauschenden Daten und Objekte für diesen Remote-Datenverteiler fest</kurzinfo>
				</info>
			</attributliste>
		</attributgruppenDefinition>
		<attributgruppenDefinition pid="atg.triggerKExDaV" name="TriggerKExDaV" parametrierend="nein">
			<info>
				<kurzinfo>Attributgruppe zum Auslösen von einmaligen Parameteraustauschen</kurzinfo>
			</info>
			<aspekt pid="asp.anfrage" onlineModus="senke">
			</aspekt>
			<attribut pid="att.text" anzahl="1" anzahlIst="variabel" name="Datenverteiler-Pid">
				<info>
					<kurzinfo>Wählt die zu triggernden Datenverteiler-Verbindungen anhand ihrer Pid aus</kurzinfo>
				</info>
			</attribut>
			<attributliste pid="atl.datenverteilerIP" anzahl="1" anzahlIst="variabel" name="Datenverteiler-IP">
				<info>
					<kurzinfo>Wählt die zu triggernden Datenverteiler-Verbindungen anhand ihrer IP aus</kurzinfo>
				</info>
			</attributliste>
			<attribut pid="att.austauschRichtung" anzahl="1" anzahlIst="fest" name="AustauschRichtung">
				<info>
					<kurzinfo>Bestimmt die Richtung des Datenaustausches</kurzinfo>
				</info>
			</attribut>
		</attributgruppenDefinition>
		<attributDefinition pid="att.austauschRichtung">
			<info>
				<kurzinfo>Richtung des Datenaustausches</kurzinfo>
			</info>
			<ganzzahl bits="8">
				<zustand name="Lokal nach Remote" wert="1">
				</zustand>
				<zustand name="Remote nach Lokal" wert="2">
				</zustand>
			</ganzzahl>
		</attributDefinition>
		<attributDefinition pid="att.onlinedatenRichtung">
			<info>
				<kurzinfo>Angabe der Richtung des Austauschs von Onlinedaten</kurzinfo>
			</info>
			<ganzzahl bits="8">
				<zustand name="Lokal Quelle -&gt; Remote Empfänger" wert="1">
				</zustand>
				<zustand name="Lokal Sender -&gt; Remote Senke" wert="2">
				</zustand>
				<zustand name="Lokal Senke &lt;- Remote Sender" wert="3">
				</zustand>
				<zustand name="Lokal Empfänger &lt;- Remote Quelle" wert="4">
				</zustand>
			</ganzzahl>
		</attributDefinition>
		<attributDefinition pid="att.parameterdatenStrategie">
			<info>
				<kurzinfo>Strategie beim Austausch von Parameterdaten</kurzinfo>
			</info>
			<ganzzahl bits="8">
				<zustand name="Lokale Verwaltung, Remote lesen" wert="1">
				</zustand>
				<zustand name="Lokale Verwaltung, Remote lesen und ändern" wert="2">
				</zustand>
				<zustand name="Remote Verwaltung, Lokal lesen" wert="3">
				</zustand>
				<zustand name="Remote Verwaltung, Lokal lesen und ändern" wert="4">
				</zustand>
				<zustand name="Verwaltung im Lokal und Remote-System" wert="5">
				</zustand>
				<zustand name="Verwaltung im Lokal und Remote-System mit Trigger" wert="6">
				</zustand>
			</ganzzahl>
		</attributDefinition>
		<attributlistenDefinition pid="atl.austauschDynamischeMengen" name="AustauschDynamischeMengen">
			<info>
				<kurzinfo>Definition zum Austausch von dynamischen Mengen</kurzinfo>
			</info>
			<attributliste pid="atl.mengenReferenzLokal" anzahl="1" anzahlIst="fest" name="DynamischeMengeLokal">
				<info>
					<kurzinfo>Lokale dynamische Menge.</kurzinfo>
				</info>
			</attributliste>
			<attributliste pid="atl.mengenReferenzRemote" anzahl="1" anzahlIst="fest" name="DynamischeMengeRemote">
				<info>
					<kurzinfo>Entfernte Dynamische Menge</kurzinfo>
				</info>
			</attributliste>
			<attribut pid="att.austauschRichtung" anzahl="1" anzahlIst="fest" name="Richtung">
				<info>
					<kurzinfo>Angabe der Richtung des Datenaustausches</kurzinfo>
				</info>
			</attribut>
