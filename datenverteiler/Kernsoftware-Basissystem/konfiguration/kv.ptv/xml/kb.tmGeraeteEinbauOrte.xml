<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE konfigurationsBereich PUBLIC "-//K2S//DTD Dokument//DE" "K2S.dtd">
<konfigurationsBereich pid="kb.tmGeraeteEinbauOrte" name="TeilmodellGeraeteEinbauOrteGlobal" verantwortlich="kv.ptv">
	<info>
		<kurzinfo>Teilmodell mit Datenmodellierung für konkrete Geraete und deren mögliche Einbauorte.</kurzinfo>
	</info>
	<konfigurationsAenderung stand="12.06.2007" version="1" autor="inovat, Dipl.-Ing. H. C. Kniß (HCK)" grund="Änderungsdokumentation siehe 'ReleaseNotes kv.ptv.doc'"></konfigurationsAenderung>
	<konfigurationsAenderung stand="13.06.2007" version="2" autor="inovat, Dipl.-Ing. H. C. Kniß (HCK)" grund="Änderungsdokumentation siehe 'ReleaseNotes kv.ptv.doc'"></konfigurationsAenderung>
	<modell>
		<typDefinition pid="typ.adressDatum" name="AdressDatum" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein Adressdatensatz</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.adressDatum"/>
		</typDefinition>
		<typDefinition pid="typ.einbauOrt" name="EinbauOrt" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein konktreter Einbauort für ein Gerät</kurzinfo>
			</info>
			<erweitert pid="typ.konkretesEtwas"/>
			<attributgruppe pid="atg.einbauOrt"/>
		</typDefinition>
		<typDefinition pid="typ.einbauOrtTyp" name="EinbauOrtTyp" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein (abstrakter) Typ von Einbauort, z.B. ein Typ von 19''-Rahmen</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.einbauOrtTyp"/>
		</typDefinition>
		<typDefinition pid="typ.geraet" name="Geraet" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein konkretes Geraet</kurzinfo>
			</info>
			<erweitert pid="typ.konkretesEtwas"/>
			<attributgruppe pid="atg.geraet"/>
			<attributgruppe pid="atg.geraetEinbauOrt"/>
		</typDefinition>
		<typDefinition pid="typ.geraeteTyp" name="GeraeteTyp" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein (abstrakter) Gerätetyp</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.geraeteTyp"/>
		</typDefinition>
		<typDefinition pid="typ.hersteller" name="Hersteller" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein Hersteller</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.hersteller"/>
			<menge pid="menge.adressDaten" name="Adressen" erforderlich="ja">
				<info>
					<kurzinfo>Adressen des Herstellers bzw. dessen Gliederung</kurzinfo>
				</info>
			</menge>
		</typDefinition>
		<typDefinition pid="typ.hochbauSchrankSchacht" name="HochbauSchrankSchacht" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein konkretes Ding (Hochbau, Schrank, Schacht..., das Einbauorte und Geräte umschliesst. Die Atributgruppe PunktLiegtAufLinienobjekt referenziert auf den Strassenabschnitt, über den dieses konkrete Ding erreichbar ist.</kurzinfo>
			</info>
			<erweitert pid="typ.konkretesEtwas"/>
			<attributgruppe pid="atg.hochbauSchrankSchacht"/>
		</typDefinition>
		<typDefinition pid="typ.hochbauSchrankSchachtTyp" name="HochbauSchrankSchachtTyp" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein Typ von Hochbau, Schrank, Schacht etc, d.h. z.B. ein Typ von Streckenstationsschrank, ein Typ von Kabelschacht, ein Typ von Kabelhaus.</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.hochbauSchrankSchachtTyp"/>
		</typDefinition>
		<typDefinition pid="typ.instandhalter" name="Instandhalter" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein Instandhalter</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.instandhalter"/>
			<menge pid="menge.adressDaten" name="Adressen" erforderlich="ja">
				<info>
					<kurzinfo>Adressen des Instandhalters bzw. dessen Gliederung</kurzinfo>
				</info>
			</menge>
		</typDefinition>
		<typDefinition pid="typ.konkretesEtwas" name="KonkretesEtwas" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Überobjekt für alle Objekte, die man konkret anfassen kann</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.konkretesEtwas"/>
		</typDefinition>
		<typDefinition pid="typ.lieferant" name="Lieferant" objektNamenPermanent="nein">
			<info>
				<kurzinfo>Ein Lieferant</kurzinfo>
			</info>
			<erweitert pid="typ.konfigurationsObjekt"/>
			<attributgruppe pid="atg.lieferant"/>
			<menge pid="menge.adressDaten" name="Adressen" erforderlich="ja">
				<info>
					<kurzinfo>Adressen des Lieferanten bzw. dessen Gliederung</kurzinfo>
				</info>
			</menge>
		</typDefinition>
		<attributgruppenDefinition pid="atg.adressDatum" name="Adressdatum" parametrierend="nein">
			<info>
				<kurzinfo>Eine gültige Adresse zu dem Hersteller / Lieferant / Instandhalter; Problem: es kann mehrere geben</kurzinfo>
			</info>
			<aspekt pid="asp.eigenschaften" konfigurationsModus="datensatzOptional">
			</aspekt>
			<attribut pid="att.text" anzahl="1" anzahlIst="fest" name="Name">
				<info>
					<kurzinfo>Der Name des Standorts</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.text" anzahl="1" anzahlIst="fest" name="Ort">
				<info>
					<kurzinfo>Der Ort des Standorts</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.text" anzahl="1" anzahlIst="fest" name="Strasse">
				<info>
					<kurzinfo>Die Strasse des Standorts</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.text" anzahl="1" anzahlIst="fest" name="Hausnummer">
				<info>
					<kurzinfo>Die Hausnummer des Standorts</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.zahlPositiv" anzahl="1" anzahlIst="variabel" name="Postleitzahl">
				<info>
					<kurzinfo>Die Postleitzahl des Standorts</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.zahlPositiv" anzahl="0" anzahlIst="variabel" name="Postfach">
				<info>
					<kurzinfo>Postfach des Standorts</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.text" anzahl="0" anzahlIst="variabel" name="Telefonnummern">
				<info>
					<kurzinfo>Telefonnummer des Standorts</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.text" anzahl="0" anzahlIst="variabel" name="Email">
				<info>
					<kurzinfo>Email des Standorts</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.text" anzahl="0" anzahlIst="variabel" name="Telefax">
				<info>
					<kurzinfo>Telefax des Standorts</kurzinfo>
				</info>
			</attribut>
		</attributgruppenDefinition>
		<attributgruppenDefinition pid="atg.einbauOrt" name="EinbauOrt" parametrierend="nein">
			<info>
				<kurzinfo>Daten eines konkreten Einbauorts</kurzinfo>
			</info>
			<aspekt pid="asp.eigenschaften" konfigurationsModus="datensatzOptional">
			</aspekt>
			<attribut pid="att.url" anzahl="0" anzahlIst="variabel" name="Dokumentation">
				<info>
					<kurzinfo>Dokumentation zum konkreten Einbauort, z.B. spezielle Schaltpläne, Zeichnungen</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.url" anzahl="0" anzahlIst="variabel" name="Bilder">
				<info>
					<kurzinfo>Bilder zum konkreten Einbauort</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.einbauOrtTypReferenzOptional" anzahl="1" anzahlIst="fest" name="EinbauOrtTyp">
				<info>
					<kurzinfo>Referenz auf den Typ, von dem dieser konkrete Einbauort ist</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.hochbauSchrankSchachtReferenz" anzahl="1" anzahlIst="fest" name="HochbauSchrankSchacht">
				<info>
					<kurzinfo>Referenz auf den Ort (Hochbau,Schrank,Schacht, in dem sich der Einbauort befindet.</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.einbauOrtReferenz" anzahl="0" anzahlIst="variabel" name="EnthalteneEinbauOrte">
				<info>
					<kurzinfo>Referenz auf weitere Einbauorte, die(direkt in diesem Einbauort enthalten sind (und die wiederum andere Einbauorte beinhalten können. Beispiel: Ein 19''-Rahmen beinhaltet mehrere Racks, die wiederum Einbauplätze für Rechner enthalten. Also 1. HierarchieebeneEinbauort 19''-Rahmen, 2. Hierarchieebene Einbauort Rack, 3.Hierarchieebene Einbauort Einschubplatz für Rechner</kurzinfo>
				</info>
			</attribut>
		</attributgruppenDefinition>
		<attributgruppenDefinition pid="atg.einbauOrtTyp" name="EinbauOrtTyp" parametrierend="nein">
			<info>
				<kurzinfo>Daten eines abstrakten Einbauorts</kurzinfo>
			</info>
			<aspekt pid="asp.eigenschaften" konfigurationsModus="datensatzOptional">
			</aspekt>
			<attribut pid="att.text" anzahl="1" anzahlIst="fest" name="Bezeichnung">
				<info>
					<kurzinfo>Bezeichnung des EinbauOrtTyps, z.B. Herstellerbezeichnung</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.einbauOrtTypReferenzOptional" anzahl="0" anzahlIst="variabel" name="ZulaessigeEinbauOrtTypen">
				<info>
					<kurzinfo>Auflistung aller Einbauorttypen, in der dieser Einbauorttyp eingebaut werden kann, ermöglicht verfizierung der Konfiguration sowie eine Unterstützung bei zukünftigen Benutzeroberflächen. Der Einbauorttyp "Lager" sollte immer enthalten sein (sofern es sich nicht um ein Lager oder ein Lagerplatz handelt).</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.url" anzahl="0" anzahlIst="variabel" name="Dokumentation">
				<info>
					<kurzinfo>Dokumentation zum abstrakten Einbauorttyp, z.B. Handbücher...</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.url" anzahl="0" anzahlIst="variabel" name="Bilder">
				<info>
					<kurzinfo>Bilder zum abstrakten Einbauorttyp</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetLaengeMeter" anzahl="1" anzahlIst="fest" name="MaxBreite">
				<info>
					<kurzinfo>Maximale Breite des Einbauorttypstyps, 0 wenn keine sinnvole Angabe möglich</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetLaengeMeter" anzahl="1" anzahlIst="fest" name="MaxHoehe">
				<info>
					<kurzinfo>Maximale Höhe des Einbauorttyps, 0 wenn keine sinnvole Angabe möglich</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetLaengeMeter" anzahl="1" anzahlIst="fest" name="MaxTiefe">
				<info>
					<kurzinfo>Maximale Tiefe des Einbauorttyps, 0 wenn keine sinnvole Angabe möglich</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetMasse" anzahl="1" anzahlIst="fest" name="Masse">
				<info>
					<kurzinfo>Masse des Einbauorttyps ohne Einbauten; , 0 wenn keine sinnvole Angabe möglich</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.herstellerReferenz" anzahl="0" anzahlIst="variabel" name="Hersteller">
				<info>
					<kurzinfo>Referenz auf den oder die Hersteller, welche diese Art von Einbauort liefern, kann leer sein, wenn diese Angabe keinen Sinn macht, z.B: bei einem Lagerplatz</kurzinfo>
				</info>
			</attribut>
		</attributgruppenDefinition>
		<attributgruppenDefinition pid="atg.geraet" name="Geraet" parametrierend="nein">
			<info>
				<kurzinfo>Daten eines konkreten Geräts</kurzinfo>
			</info>
			<aspekt pid="asp.eigenschaften" konfigurationsModus="datensatzOptional">
			</aspekt>
			<attribut pid="att.geraeteTypReferenz" anzahl="1" anzahlIst="fest" name="ZuGeraeteTyp">
				<info>
					<kurzinfo>Referenz auf den Typ, von dem dieses konkrete Gerät ist</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.text" anzahl="0" anzahlIst="variabel" name="Bezeichnung">
				<info>
					<kurzinfo>Bezeichnung dieses konkreten Geräts (Freitext)</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetReferenz" anzahl="0" anzahlIst="variabel" name="Komponenten">
				<info>
					<kurzinfo>Referenz weitere Geraete, die Komponenten dieses Geraets sind.</kurzinfo>
				</info>
			</attribut>
		</attributgruppenDefinition>
		<attributgruppenDefinition pid="atg.geraetEinbauOrt" name="EinbauOrtGerät" parametrierend="nein">
			<info>
				<kurzinfo>Aktueller Einbauort eines konkreten Geräts</kurzinfo>
			</info>
			<aspekt pid="asp.eigenschaften" konfigurationsModus="datensatzOptional">
			</aspekt>
			<attribut pid="att.einbauOrtReferenz" anzahl="1" anzahlIst="fest" name="EinbauOrt">
				<info>
					<kurzinfo>Referenz auf den Einbauort, in dem sich das Geraet gerade befindet. Dies kann auch ein LAgerraum etc. sein.Diese Referenz sollte wenn möglich nicht konfigurierend sein Es muss auf der anderen Seite immer einen Einbauort geben.</kurzinfo>
				</info>
			</attribut>
		</attributgruppenDefinition>
		<attributgruppenDefinition pid="atg.geraeteTyp" name="GeraeteTyp" parametrierend="nein">
			<info>
				<kurzinfo>Daten eines abstrakten Gerätetyps</kurzinfo>
			</info>
			<aspekt pid="asp.eigenschaften" konfigurationsModus="datensatzOptional">
			</aspekt>
			<attribut pid="att.herstellerReferenz" anzahl="1" anzahlIst="fest" name="Hersteller">
				<info>
					<kurzinfo>Referenz auf den Herstellers dieses Gerätetyps</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.text" anzahl="1" anzahlIst="fest" name="Herstellerbezeichnung">
				<info>
					<kurzinfo>Bezeichnung dieses Gerätetyps beim Hersteller</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetTemperatur" anzahl="1" anzahlIst="fest" name="MinimalTemperatur">
				<info>
					<kurzinfo>Minimale Temperatur, bei der dieses Gerät betrieben werden darf</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetTemperatur" anzahl="1" anzahlIst="fest" name="MaximalTemperatur">
				<info>
					<kurzinfo>Maximale Temperatur, bei der dieses Gerät betrieben werden darf</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetTemperatur" anzahl="1" anzahlIst="fest" name="MaximalLagerTemperatur">
				<info>
					<kurzinfo>Maximale Temperatur, bei der dieses Gerät gelagert werden darf</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetTemperatur" anzahl="1" anzahlIst="fest" name="MinimalLagerTemperatur">
				<info>
					<kurzinfo>Minimale Temperatur, bei der dieses Gerät gelagert werden darf</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.url" anzahl="0" anzahlIst="variabel" name="Dokumentation">
				<info>
					<kurzinfo>Dokumentation zum abstrakten Gerätetyp, z.B. Handbücher...</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.url" anzahl="0" anzahlIst="variabel" name="Bilder">
				<info>
					<kurzinfo>Bilder zum abstrakten Gerätetyp</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetLaengeMeter" anzahl="1" anzahlIst="fest" name="MaxBreite">
				<info>
					<kurzinfo>Maximale Breite des Gerätetyps</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetLaengeMeter" anzahl="1" anzahlIst="fest" name="MaxHoehe">
				<info>
					<kurzinfo>Maximale Höhe des Gerätetyps</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetLaengeMeter" anzahl="1" anzahlIst="fest" name="MaxTiefe">
				<info>
					<kurzinfo>Maximale Tiefe des Gerätetyps</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetMasse" anzahl="1" anzahlIst="fest" name="MasseOhneKomponenten">
				<info>
					<kurzinfo>Masse des Gerätetyps ohne eventuell dazugehörende Komponenten</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetLeistung" anzahl="1" anzahlIst="fest" name="LeistungTypisch">
				<info>
					<kurzinfo>Typische Leistungsaufnahme des Gerätetyps ohne eventuell dazugehörende Komponenten</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetLeistung" anzahl="1" anzahlIst="fest" name="LeistungMaximal">
				<info>
					<kurzinfo>Maximale Leistungsaufnahme des Gerätetyps ohne eventuell dazugehörende Komponenten</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetGehaeuseDichtigkeit" anzahl="1" anzahlIst="fest" name="Dichtigkeit">
				<info>
					<kurzinfo>Dichtigkeit des Gerätetyps</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.einbauOrtTypReferenzOptional" anzahl="1" anzahlIst="variabel" name="ZulaessigeEinbauOrtTypen">
				<info>
					<kurzinfo>Auflistung aller Einbauorttypen, in der dieser Gerätetyp eingebaut werden kann, ermöglicht verfizierung der konfiguration sowie eine Unterstützung bei zukünftigen Benutzeroberflächen. Der Einbauorttyp "Lager" sollte immer enthalten sein.</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.einbauOrtTypReferenzOptional" anzahl="0" anzahlIst="variabel" name="HatEinbauOrtTypen">
				<info>
					<kurzinfo>Auflistung aller Einbauorttypen, welches dieser Gerätetyp bereitstellt. Werden mehrere gleiche bereitgestellt, so sind mehrfach die gleichen Referenzen einzutragen.</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetSpannung" anzahl="1" anzahlIst="fest" name="NennspannungMax">
				<info>
					<kurzinfo>Maximalspannung für diesen Gerätetyp</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetSpannung" anzahl="1" anzahlIst="fest" name="NennspannungMin">
				<info>
					<kurzinfo>Minimalspannung für diesen Gerätetyp</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetSpannungsart" anzahl="1" anzahlIst="fest" name="Spannungsart">
				<info>
					<kurzinfo>Spannungsart für diesen Gerätetyp (Gleichstrom, Wechselstrom ...)</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetBeschleunigung" anzahl="1" anzahlIst="fest" name="Maximalbeschleunigung">
				<info>
					<kurzinfo>Maximalbeschleunigung für diesen Gerätetyp</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.geraetLaengeMeter" anzahl="1" anzahlIst="fest" name="Maximalhoehe">
				<info>
					<kurzinfo>Maximalhöhe über NN, in der dieser Gerätetyp betrieben werden darf</kurzinfo>
				</info>
			</attribut>
		</attributgruppenDefinition>
		<attributgruppenDefinition pid="atg.hersteller" name="Hersteller" parametrierend="nein">
			<info>
				<kurzinfo>Daten eines Herstellers</kurzinfo>
			</info>
			<aspekt pid="asp.eigenschaften" konfigurationsModus="datensatzOptional">
			</aspekt>
			<attribut pid="att.adressDatumReferenz" anzahl="1" anzahlIst="variabel" name="Adresse">
				<info>
					<kurzinfo>Die Adresse(n) des Herstellers</kurzinfo>
				</info>
			</attribut>
			<attribut pid="att.text" anzahl="1" anzahlIst="fest" name="Name">
				<info>
					<kurzinfo>Der Name des Herstellers (ggf. mit Abteilungsbezeichnung etc...)</kurzinfo>
				</info>
			</attribut>
		</attributgruppenDefinition>
		<attributgruppenDefinition pid="atg.hochbauSchrankSchacht" name="HochbauSchrankSchacht" parametrierend="nein">
			<info>
				<kurzinfo>Daten eines konkreten Hochbaus / Schranks / Schachts ...</kurzinfo>
			</info>
